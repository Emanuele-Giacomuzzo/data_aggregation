file = {:Users/ema/Google Drive/Documents/Other/Reading material/Papers downloaded/Clarke, Warwick{\_}1999{\_}The taxonomic distinctness measure of biodiversity weighting of step lengths between hierarchical levels.pdf:pdf},
isbn = {0171-8630},
issn = {01718630},
keywords = {b,island biogeography,isles of scllly,nematodes,odiversily,taxonomic distinctness},
pages = {1--15},
pmid = {21179189},
title = {{The taxonomic distinctness measure of biodiversity: weighting of step lengths between hierarchical levels}},
