doi = {10.1038/s41559-019-0899-x},
file = {:Users/ema/Google Drive/Documents/Other/Reading material/Papers downloaded/Brose et al.{\_}2019{\_}Predator traits determine food-web architecture across ecosystems.pdf:pdf},
issn = {2397334X},
journal = {Nature Ecology and Evolution},
number = {6},
