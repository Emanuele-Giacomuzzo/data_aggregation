keywords = {Reliability and validity,Research,Statistics},
number = {2},
pages = {155--163},
pmid = {27330520},
publisher = {Elsevier B.V.},
title = {{A Guideline of Selecting and Reporting Intraclass Correlation Coefficients for Reliability Research}},
