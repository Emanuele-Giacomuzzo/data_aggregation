doi = {10.2307/1352226},
file = {:Users/ema/Google Drive/Documents/Other/Reading material/Papers downloaded/Erwin{\_}1996{\_}Dependence of Waterbirds and Shorebirds on Shallow-Water Habitats in the Mid-Atlantic Coastal Region An Ecological Profile an.pdf:pdf},
issn = {01608347},
