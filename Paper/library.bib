@article{DOBSON1989,
abstract = {Abstract: Primates are among the most threatened taxa, with more than half of all species in jeopardy. In this paper we develop population models to use the kind of data on wild primates that primatologists actually collect. Our survey of recentprimate journals suggests that the average field study uses 1.5 years of data from 50 animals The models are based on the simple Leslie‚ÄêLefkovitch matrix. They suggest a simple method that allows assessment from a few years'data, of whether a population is collapsing and requires intervention To a good approximation, populations will collapse when adult survival, per inter‚Äêbirth interval, is less than 70 percent. Modifications of the basic model incorporate more realistic assumptions about social organization and densitydependent resource limitation. These allow us to identify population densities at which potential Allee effects operate, and permit more precise estimates of the minimum population sizes and compositions required for successful reintroductions to the wild The most important result is that populations of primates that live in small family groups may be more prone to ‚Äúdemographic‚Äù extinction than are more promiscuous species that live in more extended groups. Copyright {\textcopyright} 1989, Wiley Blackwell. All rights reserved},
author = {DOBSON, ANDREW P. and LEES, ANNA MARIE},
