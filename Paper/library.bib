@article{Moran2019,
abstract = {Animals are distinguished by having guts‚Äîorgans that must extract nutrients from food and also bar invasion by pathogens. Most guts are colonized by nonpathogenic microorganisms, but the functions of these microbes, or even the reasons why they occur in the gut, vary widely among animals. Sometimes these microorganisms have codiversified with hosts; sometimes they live mostly elsewhere in the environment. Either way, gut microorganisms often benefit hosts. Benefits may reflect evolutionary addiction, whereby hosts incorporate gut microorganisms into normal developmental processes. But benefits often include novel ecological capabilities; for example, many metazoan clades exist by virtue of gut communities enabling new dietary niches. Animals vary immensely in their dependence on gut microorganisms, from lacking them entirely to using them as food or to obligate dependence for development, nutrition, or protection. Many consequences of gut microorganisms for hosts can be ascribed to microbial community processes and the host's ability to shape these processes.Expected final online publication date for the Annual Review of Ecology, Evolution, and Systematics, Volume 50 is November 4, 2019. Please see http://www.annualreviews.org/page/journal/pubdates for revised estimates.},
author = {Moran, Nancy A. and Ochman, Howard and Hammer, Tobin J.},
