@article{Donoso2017,
abstract = {Defaunation by humans causes a loss of large animals in many ecosystems globally. Recent work has emphasized the consequences of downsizing in animal communities for ecosystem functioning. However, no study so far has integrated network theory and life-history trade-offs to mechanistically evaluate the functional consequences of defaunation in plant‚Äìanimal networks. Here, we simulated an avian seed-dispersal network and its derived ecosystem function seedling recruitment to assess the relative importance of different size-related mechanisms. Specifically, we considered size matching (between bird size and seed size) and size trade-offs, which are driven by differences in plant or animal species abundance (negative size‚Äì quantity relationship) as well as in recruitment probability and disperser quality (positive size‚Äìquality relationship). Defaunation led to impoverished seedling communities in terms of diversity and seed size, but only if models accounted for size matching. In addition, size trade-off in plants, in concert with size matching, provoked rapid decays in seedling abundance in response to defaunation. These results underscore a disproportional importance of large animals for ecosystem functions. Downsizing in ecological networks will have severe consequences for ecosystem functioning, especially in interaction networks that are structured by size matching between plants and animals.},
author = {Donoso, Isabel and Schleuning, Matthias and Garc{\'{i}}a, Daniel and Fr{\"{u}}nd, Jochen},
doi = {10.1098/rspb.2016.2664},
file = {:Users/ema/Google Drive/Documents/Other/Reading material/Papers downloaded/Donoso et al.{\_}2017{\_}Defaunation effects on plant recruitment depend on size matching and size trade-offs in seed-dispersal networks.pdf:pdf},
isbn = {0000000202879},
issn = {14712954},
journal = {Proceedings of the Royal Society B: Biological Sciences},
keywords = {Biodiversity‚Äìecosystem functioning,Frugivorous animals,Interaction networks,Niche model,Species traits,Trait matching},
number = {1855},
title = {{Defaunation effects on plant recruitment depend on size matching and size trade-offs in seed-dispersal networks}},
volume = {284},
year = {2017}
}
