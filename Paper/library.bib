number = {10-12},
pages = {579--590},
pmid = {17963792},
title = {{A comparative analysis of three habitat suitability models for commercial yield estimation of Tapes philippinarum in a North Adriatic coastal lagoon (Sacca di Goro, Italy)}},
volume = {55},
