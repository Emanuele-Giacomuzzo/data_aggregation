keywords = {Biodiversity,Biodiversity redistribution,Climate change,Review,Species extinctions},
mendeley-tags = {Biodiversity redistribution,Review},
number = {4},
pages = {365--377},
pmid = {22257223},
title = {{Impacts of climate change on the future of biodiversity}},
volume = {15},
year = {2012}
}
