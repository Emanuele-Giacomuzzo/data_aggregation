file = {:Users/ema/Google Drive/Documents/Other/Reading material/Papers downloaded/Moran, Ochman, Hammer{\_}2019{\_}Evolutionary and Ecological Consequences of Gut Microbial Communities.pdf:pdf},
issn = {1543-592X},
journal = {Annual Review of Ecology, Evolution, and Systematics},
keywords = {codiversification,coevolution,host filtering,microbiome,symbiosis},
number = {1},
title = {{Evolutionary and Ecological Consequences of Gut Microbial Communities}},
volume = {50},
year = {2019}
}
