doi = {10.1146/annurev-ecolsys-110316-022821},
file = {:Users/ema/Google Drive/Documents/Other/Reading material/Papers downloaded/Tylianakis, Morris{\_}2017{\_}Ecological Networks Across Environmental Gradients.pdf:pdf},
