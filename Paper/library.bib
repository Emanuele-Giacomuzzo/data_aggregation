isbn = {9780081009789},
issn = {00652504},
journal = {Advances in Ecological Research},
keywords = {Barcoding,Biodiversity,Ecological network,Ecological networks,Food web,Machine learning,Microbial network,Network inference,Next-generation sequencing,Species interactions},
mendeley-tags = {Ecological networks},
pages = {1--39},
title = {{Learning Ecological Networks from Next-Generation Sequencing Data}},
volume = {54},
year = {2016}
}
