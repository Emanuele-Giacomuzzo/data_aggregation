file = {:Users/ema/Google Drive/Documents/Other/Reading material/Papers downloaded/Vali{\~{n}}as et al.{\_}2018{\_}Global change effects on plankton community structure and trophic interactions in a Patagonian freshwater eutrophic.pdf:pdf},
issn = {15735117},
journal = {Hydrobiologia},
keywords = {Acidification,Nutrients,Phytoplankton,Solar radiation,Zooplankton},
