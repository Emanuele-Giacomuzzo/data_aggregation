journal = {Nature Geoscience},
number = {3},
pages = {191--194},
publisher = {Nature Publishing Group},
title = {{Predator-induced reduction of freshwater carbon dioxide emissions}},
url = {http://dx.doi.org/10.1038/ngeo1734},
volume = {6},
year = {2013}
}
