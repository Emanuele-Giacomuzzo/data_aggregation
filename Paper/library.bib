keywords = {Ecological networks,Predictions,Stability},
mendeley-tags = {Ecological networks,Predictions,Stability},
number = {7156},
pages = {925--928},
title = {{Non-random coextinctions in phylogenetically structured mutualistic networks}},
volume = {448},
year = {2007}
}
