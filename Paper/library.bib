file = {:Users/ema/Google Drive/Documents/Other/Reading material/Papers downloaded/Lee, Shepley, Huang{\_}2009{\_}Evaluation of off-leash dog parks in Texas and Florida A study of use patterns, user satisfaction, and percepti.pdf:pdf},
isbn = {0169-2046},
