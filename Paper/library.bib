issn = {0012-9658},
journal = {ECOLOGY},
keywords = {Biodiversity,Ecosystem Functioning,Food-Web Structure,Insurance Hypothesis,Niche Partitioning,Parasitism,Phylogenetic Species Variability,Spatial Variability,Stability,Temporal Variability},
