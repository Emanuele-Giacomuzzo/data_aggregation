journal = {Ecology Letters},
keywords = {2002,402,411,5,clustering,community composition,ecology letters,fd,functional dendrogram,functional diversity,functional groups,functional redundancy,species identity,species richness,traits},
pages = {402--411},
title = {{Functional diversity (FD), species richness and community composition}},
url = {https://onlinelibrary.wiley.com/doi/pdf/10.1046/j.1461-0248.2002.00339.x http://www04.sub.su.se:2104/pdf13{\_}15/pdf/2002/EU6/01May02/6631446.pdf?T=P{\&}P=AN{\&}K=6631446{\&}S=R{\&}D=aph{\&}EbscoContent=dGJyMNXb4kSep7c4y9f3OLCmr0ieqK5Ssau4S7aWxWXS{\&}ContentCustomer=dGJyMPGpt},
volume = {5},
