issn = {1461023X},
journal = {Ecology Letters},
keywords = {Community ecology,Mass effects,Metacommunity,Neutral model,Patch dynamics,Species sorting},
mendeley-tags = {Community ecology},
number = {7},
pages = {601--613},
title = {{The metacommunity concept: A framework for multi-scale community ecology}},
volume = {7},
year = {2004}
}
